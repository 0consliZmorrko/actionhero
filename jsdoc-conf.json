{
  "source": {
    "include": [
      "index.js",
      "initializers",
      "classes",
      "client/ActionheroWebsocketClient.jsdoc",
      "bin",
      "config"
    ],
    "exclude": [
      "test",
      "bin/templates"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs",
    "template": "node_modules/docdash-actionhero",
    "tutorials": "./tutorials",
    "readme": "./README.md",
    "recurse": false,
    "verbose": true
  },
  "templates": {
    "default": {
      "includeDate": false,
      "staticFiles": {
        "include": [ "./tutorials/images" ]
      }
    }
  },
  "plugins": [
    "plugins/markdown"
  ],
  "docdash": {
    "meta": {
      "title": "ActionHero Documentation",
      "keyword": "ActionHero Node Framework Docs Documentation",
      "description": "Documentation for the ActionHero.js Node.js framework"
    },
    "search": true,
    "collapse": true,
    "typedefs": false,
    "sectionOrder": [
      "Tutorials",
      "Classes",
      "Modules",
      "Externals",
      "Events",
      "Namespaces",
      "Mixins",
      "Interfaces"
    ]
  }
}
