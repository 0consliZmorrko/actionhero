{
  "source": {
    "include": [
      "index.js",
      "initializers",
      "classes",
      "bin",
      "config"
    ],
    "exclude": [
      "test",
      "bin/templates"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs/out",
    "template": "node_modules/docdash",
    "tutorials": "./docs/tutorials",
    "readme": "./README.md",
    "recurse": true,
    "verbose": true
  },
  "plugins": ["plugins/markdown"]
}
