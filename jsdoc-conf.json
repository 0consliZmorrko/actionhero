{
  "source": {
    "include": [
      "index.js",
      "initializers",
      "classes",
      "bin",
      "config"
    ],
    "exclude": [ "test" ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs/out",
    "tutorials": "./docs/tutorials",
    "readme": "./README.md"
  },
  "plugins": ["plugins/markdown"]
}
